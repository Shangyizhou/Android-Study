<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="TabLayoutTextStyle">
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/grey</item>
    </style>

    <style name="TabLayoutTextSelected">
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="TabLayoutTextUnSelected">
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/grey</item>
    </style>

    <style name="EditTextTheme">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:ems">10</item>
        <item name="android:inputType">text</item>
        <item name="android:background">@drawable/shape_edit_whit_bg</item>
        <item name="android:gravity">top|left</item>
        <item name="android:textColorHint">@color/gray</item>
    </style>

    <style name="greyCircleTheme">
        <item name="android:background">@drawable/shape_edit_grey_bg</item>
    </style>

    <style name="AutoAiButton">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:background">@drawable/shape_edit_bg</item>
        <item name="android:gravity">top|left</item>
        <item name="android:textColorHint">@color/gray</item>
    </style>

</resources>